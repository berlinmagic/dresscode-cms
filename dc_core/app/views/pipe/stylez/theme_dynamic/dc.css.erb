html, body, body > * { margin:0; padding:0; border:0; vertical-align: top; }


.clearfix:before,.clearfix:after{content:'';display:table}.clearfix:after{clear:both}.clearfix{zoom:1}


.field { margin: 0 0 13px; }

.LB_Box { min-width: 300px; max-width: 800px; min-height: 200px; max-height: 650px; padding: 20px;}




.absolute_actions { 
	position: absolute; right: 13px; top: 13px; background: #f3f2f2; padding: 10px 30px 15px; text-align: right;
	-khtml-border-radius: 	 10px;		-webkit-border-radius: 	 10px;
	-moz-border-radius: 	 10px;		-o-border-radius: 		 10px;
	-ms-border-radius: 		 10px;		border-radius: 			 10px;
	display:block; border: solid 1px #ccc;
	}
.right_actions { float: right; background: #f3f2f2; padding: 0 20px;}

#layout_content { border: dashed 1px #555; padding: 10px; position:relative; z-index: 0; overflow-x: hidden; overflow-y: auto; }


.f_right { float: right; }

.f_right.push_1 { left:0; margin-right: 25px; }
.f_right.push_2 { left:0; margin-right: 45px; }
.f_right.push_3 { left:0; margin-right: 65px; }
.f_right.push_4 { left:0; margin-right: 85px; }
.f_right.push_5 { left:0; margin-right: 105px; }


.tl_element { outline: dotted 1px #777; background: #fff; overflow: hidden; position:relative; padding: 0 0 3px; 
 	display:block;}
.tl_element.green { background: #aaf4fa; outline: dotted 2px #c00; }

.tl_element.in_der_mache { background: #aaf4fa; outline: dotted 3px #c00; }

.platzhalter { background: #aaf4fa; outline: dotted 3px #c00; min-height: 50px !important; display: block !important; visibility: visible; position: relative; }

.platzhalter.mrow { margin: 1px 0 13px; }

.e_sub_box { padding: 5px; }

.tlrow { margin-bottom: 15px; background: url("/images/back.png"); }

.inna_container { margin: 0 -6px 5px -5px; background: rgba(190,100,30,.1); }


.element_head { padding: 3px 10px; color: #666; font-size: 12px; line-height: 20px; }


#example_wrapper { border: dashed 1px #555; padding: 10px 10px 20px; position:relative; z-index: 0; overflow-x: hidden; overflow-y: auto; background: #333;}

.example_row { padding: 5px 0; background: rgba(199,199,199,.7);}
.example_cell { outline: dotted 1px #777; padding: 5px 0; min-height: 20px; text-align: center; background: #fff; }

.ex_widget { padding: 5px; margin: 5px; background: #333; color: #ffc;}


nav.system_nav { text-align: center; }
nav.system_nav a { text-decoration: none; }


tr.more_set { display: none; }
#mehr_user_sets { text-decoration: none; color: #555;}
#login_help { text-decoration: none; color: #555;}
#login_help_links { display: none; }

