<%= form_for([:dc, @page]) do |f| %>
	<%= f.error_messages %>
	<br/>
	
	Seiten-Art:<br/>
	<%= f.select :site_type, options_for_select( Page::SITE_TYPES.map {|c| [c,c]}, @page.site_type )%><br/>
	<br/>
	
	Seiten-Parameter:<br/>
	<%= f.select :type_param, options_for_select( Page::SITE_TYPE_PARAMS.map {|c| [c,c]}, @page.type_param )%><br/>
	<br/>
	
	
	
	System-Name:<br/>
	<%= f.text_field :system_name %><br/>
	<br/>
	
	Seiten-Slug:<br/>
	<%= f.text_field :slug %><br/>
	<br/>
	
	
	<br/>
	<%= f.submit ' update! ' %>
	<br/>
	<br/>
<% end %>

