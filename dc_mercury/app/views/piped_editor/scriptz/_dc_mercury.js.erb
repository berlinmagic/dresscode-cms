<%= raw( render( "piped_editor/scriptz/dc_mercury/dependencies/showdown" ) ) %>
/*
 *
 *
 * Require DcMercury Editor itself.
 */
<%= raw( render( "piped_editor/scriptz/dc_mercury/dc_mercury" ) ) %>
/*
 *
 *
 *  Require all dc_mercury files
 */
